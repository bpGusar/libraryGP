  },
  "scripts": {
    "start": "concurrently \"node scripts/start.js\" \"cd backend && yarn watch\"",
    "build": "node scripts/build.js",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "check-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,json,scss,css}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@babel/core": "7.6.0",
    "@svgr/webpack": "4.3.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
