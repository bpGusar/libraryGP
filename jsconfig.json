{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "preserve",
    "paths": {
      "@src/*": ["./src/*"],
      "@axios/*": ["./src/config/axios/*"],
      "@UI/*": ["./src/views/UI/*"],
      "@views/*": ["./src/views/*"],
      "@DUI/*": ["./src/views/Dashboard/*"],
      "@act/*": ["./src/common/store/action/*"],
      "@store/*": ["./src/common/store/index/*"],
      "@utils/*": ["./src/common/utils/*"],
      "@msg/*": ["./src/common/messages/*"]
    },
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}
